{
  "name": "graphql-boilerplate",
  "version": "0.1.0",
  "author": "Audiovisuaali",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "dev": "nodemon --ext ts,json,graphql --exec babel-node --extensions '.ts' -r dotenv/config app/index.ts | ./node_modules/.bin/bunyan",
    "dev:debug": "DEBUG=knex:query yarn dev",
    "codegen": "graphql-codegen && yarn codegen:disable-rules && yarn codegen:lint",
    "codegen:lint": "eslint app/generation/generated.ts --fix",
    "codegen:disable-rules": "ts-node ./scripts/add-comments-to-generated.ts",
    "lint": "eslint . --ext ts,js",
    "test": "jest",
    "typescript-check": "yarn tsc",
    "database:migrate:latest": "knex migrate:latest",
    "database:migrate:down": "knex migrate:down",
    "seed": "knex seed:run",
    "validate-schema": "ts-node --files -r tsconfig-paths/register ./scripts/validate-schema.ts"
  },
  "dependencies": {
    "apollo-datasource": "^0.7.3",
    "apollo-server-express": "^2.21.0",
    "apollo-server-plugin-response-cache": "^0.6.0",
    "aws-sdk": "^2.858.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "finnish-ssn": "^2.0.4",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.8.0",
    "graphql-tools": "^7.0.4",
    "graphql-validation-complexity": "^0.4.2",
    "helmet": "^4.4.1",
    "http-proxy-middleware": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.1",
    "libphonenumber-js": "^1.9.12",
    "luxon": "^1.26.0",
    "moment": "^2.29.1",
    "pg": "^8.5.1",
    "serverless-http": "^2.7.0",
    "serverless-prune-plugin": "^1.4.4",
    "socket.io": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/enzyme": "^3.10.8",
    "@types/faker": "^5.1.7",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^1.26.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/socket.io": "^2.1.13",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "copy-webpack-plugin": "^8.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "module-resolver": "^1.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-vpc-plugin": "^1.0.2",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack-node-externals": "^2.5.2"
  }
}
