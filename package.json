{
  "name": "graphql-boilerplate",
  "version": "0.1.0",
  "author": "Audiovisuaali",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "dev": "webpack --config webpack.dev.js --watch",
    "dev:debug": "DEBUG=knex:query webpack --config webpack.dev.js --watch",
    "build": "webpack --config webpack.prod.js",
    "build:serverless": "serverless package",
    "lint": "eslint . --ext ts,js",
    "test": "jest --forceExit --detectOpenHandles",
    "script": "ts-node --files -r tsconfig-paths/register",
    "knex": "ts-node -r tsconfig-paths/register node_modules/.bin/knex",
    "codegen": "graphql-codegen && yarn codegen:disable-rules && yarn codegen:lint",
    "codegen:lint": "eslint app/generation/generated.ts --fix",
    "codegen:disable-rules": "npm run script -- ./scripts/add-comments-to-generated.ts",
    "validate-schema": "npm run script -- ./scripts/validate-schema.ts",
    "typescript-check": "yarn tsc",
    "database:migrate:latest": "npm run knex -- migrate:latest",
    "database:migrate:rollback": "npm run knex -- migrate:rollback --all",
    "seed": "npm run knex -- seed:run",
    "validate": "npm ls"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.14.0",
    "apollo-server-express": "^2.25.0",
    "apollo-server-plugin-response-cache": "^0.9.0",
    "aws-sdk": "^2.918.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "finnish-ssn": "^2.0.4",
    "fp-ts": "^2.10.5",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.9.3",
    "graphql-tools": "^7.0.5",
    "graphql-validation-complexity": "^0.4.2",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.0",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^1.2.2",
    "iso-3166-ts": "^2.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "libphonenumber-js": "^1.9.19",
    "luxon": "^1.27.0",
    "pg": "^8.6.0",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@graphql-eslint/eslint-plugin": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/enzyme": "^3.10.8",
    "@types/faker": "^5.5.5",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/luxon": "^1.26.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.6.1",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.0",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "faker": "^5.5.3",
    "jest": "^27.0.1",
    "nock": "^13.0.11",
    "nodemon-webpack-plugin": "^4.5.2",
    "prettier": "^2.3.0",
    "serverless": "^2.43.1",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-prune-plugin": "^1.5.0",
    "serverless-vpc-plugin": "^1.0.3",
    "serverless-webpack": "^5.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
